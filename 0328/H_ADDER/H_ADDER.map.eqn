--A1L6 is S~0
--operation mode is normal

A1L6 = A $ B;


--A1L4 is C_out~0
--operation mode is normal

A1L4 = A & B;


--A is A
--operation mode is input

A = INPUT();


--B is B
--operation mode is input

B = INPUT();


--S is S
--operation mode is output

S = OUTPUT(A1L6);


--C_out is C_out
--operation mode is output

C_out = OUTPUT(A1L4);


